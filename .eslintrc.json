{
  "env": {
    "node": true,
    "commonjs": true,
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:react/recommended", "eslint:recommended", "plugin:flowtype/recommended", "plugin:jsx-a11y/recommended", "plugin:flowtype-errors/recommended", "plugin:flowtype/recommended"],
  "plugins": [
    "jsx-a11y", "flowtype"
  ],
  "rules": {
    "no-unsafe-finally": "off",
    "jsx-a11y/no-noninteractive-element-to-interactive-role": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "flowtype": {
      "onlyFilesWithFlowAnnotation": true
    },
    "import/resolver": {
      "node": {
        "extensions": [".js",".jsx"]
      }
    }
  }
}